{
  "name": "werewolf-hubot",
  "version": "0.0.0",
  "private": true,
  "author": "伊吹英晃 <ibuki-90@hotmail.co.jp>",
  "description": "A simple helpful robot for your Company",
  "scripts": {
    "build": "tsc",
    "start": "hubot --name \"werewolf-hubot\"",
